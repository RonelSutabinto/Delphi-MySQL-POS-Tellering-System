  object TotalCollection: TMyQuery
    Connection = BillingData.MyConnection1
    SQL.Strings = (
      'SELECT sundries ORNumber, '
      '       collection.collectorcode code, '
      '       collector.name name,'
      '       sum(totalbill) totalbill, '
      '       sum(ucec) ucec, '
      '       sum(ucme) ucme, '
      '       sum(vatgenco) GenCo, '
      '       sum(vattransco) TransCo, '
      '       sum(vatdist) Dist, '
      '       sum(vatsystemlossgenco) SystemLoss,'
      
        '       sum(totalbill - (ucec+ucme+vatgenco+vattransco+vatdist+va' +
        'tsystemlossgenco)) net'
      'FROM collection '
      
        'join collector on (collector.collectorcode = collection.collecto' +
        'rcode)'
      'where datepaid = :datepaid '
      'group by sundries'
      'order by sundries')
